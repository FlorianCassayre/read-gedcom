{
  "name": "read-gedcom",
  "description": "Gedcom file reader",
  "version": "0.3.1",
  "main": "dist/cjs/index.js",
  "module": "dist/es6/index.js",
  "types": "dist/cjs/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "keywords": [
    "gedcom",
    "genealogy"
  ],
  "author": "Florian Cassayre",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig-build.json && tsc -p tsconfig-build-es6.json && webpack --mode production",
    "build:clean": "rimraf dist && npm run build",
    "docs": "typedoc --tsconfig tsconfig-build.json",
    "test": "mocha -r ts-node/register tests/**/*.test.ts",
    "lint": "npx eslint src/ tests/ --ext .ts --fix",
    "patch": "patch-package"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:arbre-app/read-gedcom.git"
  },
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.10",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "chai": "^4.3.4",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.1",
    "eslint-plugin-standard": "^5.0.0",
    "mocha": "^9.1.3",
    "patch-package": "^6.4.7",
    "prettier": "^2.5.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "typedoc": "^0.22.10",
    "typedoc-plugin-pages-fork-fork": "0.0.3",
    "typescript": "^4.5.2",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1"
  }
}
